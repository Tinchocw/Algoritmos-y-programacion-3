!CombatienteHumanoide methodsFor: 'private' stamp: 'MC 6/30/2023 18:12:36' prior: 50608655 overrides: 50605462!
actuarEstandoEnCombate
	| enemigoElegido enemigosActivos |	
	enemigosActivos := self enemigosEnCombate.
	enemigosActivos isEmpty ifTrue: [^self].
	enemigoElegido := estrategia value: enemigosActivos.
	
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		(self puntosDeVidaPerdidos <= 8)ifTrue:[dañoAInfligirPorArma := dañoAInfligirPorArma + (4*pv)].
		].
	
	self actuarSobre: enemigoElegido.

! !

