!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 18:31:15' prior: 50605747 overrides: 50605484!
actuarSobre: unCombatiente
	
	self estaFueraDeCombate ifTrue: [^self].
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		(self puntosDeVidaPerdidos >= (8*pv))ifTrue:[
			dañoAInfligirPorArma :=  dañoAInfligirPorArma + (4*pv).
		].
	].
	
	unCombatiente recibirDaño: self dañoAInfligir.! !

