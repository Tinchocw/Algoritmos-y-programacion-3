!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 20:33:47' prior: 50622212 overrides: 50611065!
recibirDaño: unaCantidadDeDaño  
	| dañoADescontar |
	dañoADescontar := (unaCantidadDeDaño - self dañoAAbsorberPorArmadura) max: 0*pv.
	
	
	
	(estiloDeCombate = #Derviche) ifTrue:[
		(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRecibidos <= 4 ]) ifTrue:[
			dañoADescontar := dañoADescontar -(4*pv).
			].
		
	].
	self actualizarAtaquesRecibidos.
	puntosDeVidaPerdidosTotales := puntosDeVidaPerdidosTotales + dañoADescontar.
	puntosDeVida := puntosDeVida - dañoADescontar.! !
!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 20:34:36'!
aumentarPuntosDeVidaPerdidosTotales: dañoADescontar

	^ puntosDeVidaPerdidosTotales := puntosDeVidaPerdidosTotales + dañoADescontar! !
!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 20:34:36' prior: 50623532 overrides: 50611065!
recibirDaño: unaCantidadDeDaño  
	| dañoADescontar |
	dañoADescontar := (unaCantidadDeDaño - self dañoAAbsorberPorArmadura) max: 0*pv.
	
	
	
	(estiloDeCombate = #Derviche) ifTrue:[
		(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRecibidos <= 4 ]) ifTrue:[
			dañoADescontar := dañoADescontar -(4*pv).
			].
		
	].
	self actualizarAtaquesRecibidos.
	self aumentarPuntosDeVidaPerdidosTotales: dañoADescontar.
	puntosDeVida := puntosDeVida - dañoADescontar.! !

