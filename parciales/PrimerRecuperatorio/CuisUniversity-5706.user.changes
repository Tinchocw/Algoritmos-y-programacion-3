test01CombatienteHumanoideEstaConEstiloDeCombateMarcialYNoCambiaLaFormaDeCombatirALoLargoDeTodoElCombate
	
	| combatienteAtacante combatienteEnemigo |
	
	combatienteAtacante  := CombatienteHumanoide conEstilo: #Marcial.
	combatienteEnemigo  := CombatienteHumanoide conEstilo: #Marcial.
	
	combatienteAtacante enemigos: {combatienteEnemigo}.
	combatienteEnemigo enemigos: {combatienteAtacante}.
	
	combatienteAtacante recibirDaño: 10.
	combatienteEnemigo recibirDaño: 9.
	
	combatienteAtacante recibirDaño: 10.
	
	self assert: 0 * pv equals: combatienteAtacante puntosDeVida.
	self assert: 11 * pv equals: combatienteEnemigo puntosDeVida.
	
	! !

