Combatiente subclass: #CombatienteHumanoide
	instanceVariableNames: 'dañoAInfligirPorArma dañoAAbsorberPorArmadura estrategia estiloDeCombate puntosDeVidaPerdidosTotales cantidadDeAtaquesRealizados'
	classVariableNames: ''
	poolDictionaries: ''
