!CombatienteHumanoide methodsFor: 'initialization' stamp: 'MC 6/30/2023 19:13:01' prior: 50612673 overrides: 50610734!
initialize
	super initialize.
	estrategia := self estrategiaACualquiera.	
	dañoAInfligirPorArma := 2*pv.
	dañoAAbsorberPorArmadura := 0.	
	puntosDeVidaPerdidosTotales := 0.
	cantidadDeAtaquesRealizados := 0.
	posturaDeCombate := #PosturaNormal.
	! !
!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 19:13:45' prior: 50614453 overrides: 50605484!
actuarSobre: unCombatiente
	|dañoTemporal|
	dañoTemporal  := 0.
	self estaFueraDeCombate ifTrue: [^self].
	cantidadDeAtaquesRealizados := cantidadDeAtaquesRealizados + 1.
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		
	(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados <= 4])ifTrue:[
			posturaDeCombate := #PosturaFurioso.
			dañoTemporal :=  dañoAInfligirPorArma + (4*pv).
			unCombatiente recibirDaño: dañoTemporal.
			^self.
			].
		].
	
	unCombatiente recibirDaño: self dañoAInfligir.! !
!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 19:18:03' prior: 50614832 overrides: 50605484!
actuarSobre: unCombatiente
	|dañoTemporal|
	dañoTemporal  := 0.
	self estaFueraDeCombate ifTrue: [^self].
	cantidadDeAtaquesRealizados := cantidadDeAtaquesRealizados + 1.
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		
	(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados <= 4])ifTrue:[
			posturaDeCombate := #PosturaFurioso.
			dañoTemporal :=  dañoAInfligirPorArma + (4*pv).
			unCombatiente recibirDaño: dañoTemporal.
			^self.
			].
		
	(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados >= 4])ifTrue:[
		posturaDeCombate := #PosturaLevementeCansado.
		dañoTemporal :=  dañoAInfligirPorArma - (2*pv).
		unCombatiente recibirDaño: dañoTemporal.
		^self.
		].
	].
	
	unCombatiente recibirDaño: self dañoAInfligir.! !

