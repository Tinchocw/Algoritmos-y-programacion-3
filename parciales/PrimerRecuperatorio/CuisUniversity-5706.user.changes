test06CombatienteHumanoideEstaConEstiloDeCombateBarbaroRealizaCuatroAtaquesYPasaALevementeCansadoMuchasVeces
	
	| combatienteAtacante combatienteEnemigo |
	
	combatienteAtacante  := CombatienteHumanoide conEstilo: #Barbaro.
	combatienteEnemigo  := CombatienteHumanoide conEstilo: #Marcial.
	
	combatienteAtacante enemigos: {combatienteEnemigo}.
	
	combatienteAtacante recibirDaño: 8*pv.
	combatienteAtacante recibirCuracion: 2*pv.
	
	3 timesRepeat: [combatienteAtacante actuar].
	combatienteEnemigo recibirCuracion: (18*pv).
	2 timesRepeat: [combatienteAtacante actuar].
	
	
	self assert:  14* pv equals: combatienteEnemigo puntosDeVida.
	
	! !
