!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 19:08:43' prior: 50613132 overrides: 50605484!
actuarSobre: unCombatiente
	|dañoTemporal|
	dañoTemporal  := 0.
	self estaFueraDeCombate ifTrue: [^self].
	cantidadDeAtaquesRealizados := cantidadDeAtaquesRealizados + 1.
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados <= 4])ifTrue:[
			dañoTemporal :=  dañoAInfligirPorArma + (4*pv).
			unCombatiente recibirDaño: dañoTemporal.
			].
		].
	
	unCombatiente recibirDaño: self dañoAInfligir.! !

