!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 20:09:10' prior: 50620321 overrides: 50611065!
recibirDaño: unaCantidadDeDaño  
	| dañoADescontar |
	dañoADescontar := (unaCantidadDeDaño - self dañoAAbsorberPorArmadura) max: 0*pv.
	
	
	
	(estiloDeCombate = #Derviche) ifTrue:[
		((puntosDeVidaPerdidosTotales*pv) >= (8*pv)) ifTrue:[
			dañoADescontar := dañoADescontar -(4*pv).
			].
		
	].
	puntosDeVidaPerdidosTotales := puntosDeVidaPerdidosTotales + dañoADescontar.
	puntosDeVida := puntosDeVida - dañoADescontar.! !

