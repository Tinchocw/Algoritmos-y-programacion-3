!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 19:23:38' prior: 50615230 overrides: 50605484!
actuarSobre: unCombatiente
	|dañoTemporal|
	dañoTemporal :=  dañoAInfligirPorArma.
	self estaFueraDeCombate ifTrue: [^self].
	cantidadDeAtaquesRealizados := cantidadDeAtaquesRealizados + 1.
	
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		
	(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados < 4])ifTrue:[
			posturaDeCombate := #PosturaFurioso.
			dañoTemporal :=  dañoTemporal + (4*pv).
			"unCombatiente recibirDaño: dañoTemporal.
			^self."
			].
		
	(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados > 4])ifTrue:[
		posturaDeCombate := #PosturaLevementeCansado.
		dañoTemporal :=  dañoTemporal - (2*pv).
		"unCombatiente recibirDaño: dañoTemporal.
		^self."
		].
	].
	
	unCombatiente recibirDaño: dañoTemporal.! !
