!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 18:58:58' prior: 50610743 overrides: 50605484!
actuarSobre: unCombatiente
	
	self estaFueraDeCombate ifTrue: [^self].
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados <= 4])ifTrue:[
			dañoAInfligirPorArma :=  dañoAInfligirPorArma + (4*pv).
			].
		].
	
	unCombatiente recibirDaño: self dañoAInfligir.! !
!CombatienteHumanoide methodsFor: 'initialization' stamp: 'MC 6/30/2023 18:59:12' prior: 50611618 overrides: 50610734!
initialize
	super initialize.
	estrategia := self estrategiaACualquiera.	
	dañoAInfligirPorArma := 2*pv.
	dañoAAbsorberPorArmadura := 0.	
	puntosDeVidaPerdidosTotales := 0.
	cantidadDeAtaquesRealizados := 0.! !
!CombatienteHumanoide methodsFor: 'initialization' stamp: 'MC 6/30/2023 18:59:21' prior: 50611887!
initializeConEstilo: unEstiloDeCombate
	super initialize.
	estrategia := self estrategiaACualquiera.	
	dañoAInfligirPorArma := 2*pv.
	dañoAAbsorberPorArmadura := 0.	
	puntosDeVidaPerdidosTotales := 0.
	estiloDeCombate := unEstiloDeCombate.
	cantidadDeAtaquesRealizados := 0.! !
!CombatienteHumanoide methodsFor: 'combate' stamp: 'MC 6/30/2023 19:00:41' prior: 50612659 overrides: 50605484!
actuarSobre: unCombatiente
	
	self estaFueraDeCombate ifTrue: [^self].
	cantidadDeAtaquesRealizados := cantidadDeAtaquesRealizados + 1.
	(estiloDeCombate = #Barbaro) ifTrue:[ 
		(puntosDeVidaPerdidosTotales >= (8*pv) and:[cantidadDeAtaquesRealizados <= 4])ifTrue:[
			dañoAInfligirPorArma :=  dañoAInfligirPorArma + (4*pv).
			].
		].
	
	unCombatiente recibirDaño: self dañoAInfligir.! !

